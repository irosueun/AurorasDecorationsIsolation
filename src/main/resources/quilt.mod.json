{
  "schema_version": 1,
  "quilt_loader": {
    "group": "dev.lambdaurora",
    "id": "aurorasdeco",
    "version": "${version}",
    "metadata": {
      "name": "Aurora's Decorations",
      "description": "A decorations-focused mod which tries to have a twist for its different features.",
      "contributors": {
        "Aurora (Lambda Foxes)": "Author",
        "Lavender (Lambda Foxes)": "Author",
        "april": "Artist",
        "harpsi": "Artist",
        "Kat": "Artist",
        "unascribed": "Artist"
      },
      "contact": {
        "homepage": "https://lambdaurora.dev/AurorasDecorations/",
        "sources": "https://github.com/LambdAurora/AurorasDecorations.git",
        "issues": "https://github.com/LambdAurora/AurorasDecorations/issues"
      },
      "license": "LGPL-3.0-only",
      "icon": "assets/aurorasdeco/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "dev.lambdaurora.aurorasdeco.AurorasDeco"
      ],
      "client_init": [
        "dev.lambdaurora.aurorasdeco.client.AurorasDecoClient"
      ],
      "emi": [
        "dev.lambdaurora.aurorasdeco.hook.EmiHooks"
      ]
    },
    "depends": [
      {
        "id": "minecraft",
        "versions": ">=1.19.2"
      },
      {
        "id": "quilt_loader",
        "versions": ">=0.17.4"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0-beta.10"
      },
      {
        "id": "java",
        "versions": ">=17"
      },
      "quilt_block_entity",
      "quilt_biome",
      "quilt_recipe",
      "quilt_registry",
      {
        "id": "terraform-wood-api-v1",
        "versions": ">=4.1.0"
      },
      {
        "id": "trinkets",
        "versions": ">=3.4.0",
        "optional": true
      },
      {
        "id": "emi",
        "optional": true
      }
    ]
  },
  "mixin": "aurorasdeco.mixins.json"
}
